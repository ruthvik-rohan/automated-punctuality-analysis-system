BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "logs" (
	"id"	INTEGER,
	"person_id"	TEXT,
	"name"	TEXT,
	"timestamp"	TEXT,
	"status"	TEXT,
	PRIMARY KEY("id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "punctuality_reports" (
	"id"	INTEGER,
	"person_id"	TEXT,
	"name"	TEXT,
	"date"	TEXT,
	"late"	INTEGER,
	"early_exit"	INTEGER,
	"long_breaks"	INTEGER,
	PRIMARY KEY("id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "schedule" (
	"id"	INTEGER,
	"person_id"	TEXT,
	"class_start"	TEXT,
	"class_end"	TEXT,
	PRIMARY KEY("id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "users" (
	"id"	INTEGER,
	"person_id"	TEXT UNIQUE,
	"name"	TEXT,
	"role"	TEXT,
	"encoding"	BLOB,
	PRIMARY KEY("id" AUTOINCREMENT)
);
